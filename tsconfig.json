{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@playwright/test": ["tests/shared/testWithLogs"],
      "/component/*": ["src/component/*"],
      "/*": ["src/*"]
    },
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowJs": true,
    "checkJs": true,
    "noEmit": true
  },
  "include": ["src", "tests", "src/global.d.ts", "src/types/global.d.ts"]
}
